    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webp-webpack-plugin": "^3.3.1",
    "mini-css-extract-plugin": "^0.9.0",
